<configuration debug="true">
   <jmxConfigurator /><!-- Start jconsole on command line -->

   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
      <layout class="ch.qos.logback.classic.PatternLayout">
         <Pattern>%-4level:%logger{0}:[%thread]:> %msg%n</Pattern>
         <!--<Pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</Pattern> -->
      </layout>
   </appender>
   
   
   
   <logger name="se.umu.cs.jsgajn.gcom.messageordering">
      <level value="DEBUG" />
   </logger>

   <!-- if system property logback.root.level is set use that otherwise use supplied -->
   <root>
      <!-- <level value="INFO"/> -->
      <level value="${logback.root.level:-INFO}"/>
      <appender-ref ref="STDOUT" />
   </root>
</configuration>